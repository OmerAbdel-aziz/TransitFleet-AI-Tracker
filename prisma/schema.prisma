generator client {
  provider = "prisma-client-js"
 
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}




model BusHistory {
  id        String   @id @default(cuid())
  busId     String
  route     String
  lat       Float?
  lng       Float?
  eta       DateTime?
  delay     Int?
  timestamp DateTime @default(now())

  @@index([busId])
  @@index([route])
}
